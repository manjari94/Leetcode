import sys
class Solution:
    def searchInsert(self, nums: List[int], target: int) -> int:
        for i in range(0,len(nums)):
            if(target<min(nums)):
                return 0
            if(target>max(nums)):
                return(len(nums))
            if(nums[i]==target):
                return(i)
            if(nums[i]<target and nums[i+1]>target):
                return(i+1)
            

                    
                    
